{
    "compilerOptions": {
        "target": "es6",
        "module": "ES2020",
        "moduleResolution": "node",
        "lib": [ "es2017", "DOM" ],
        "esModuleInterop": true,
        "strict": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "skipLibCheck": true
    }
}
